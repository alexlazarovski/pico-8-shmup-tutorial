function _init()
 cls(0)

 startscreen()
 blinkt = 1
 t = 0
end

function _update()
 t += 1
 blinkt += 1

 if mode == "game" then
  update_game()
 elseif mode == "start" then
  update_start()
 elseif mode == "wavetxt" then
  update_wavetxt()
 elseif mode == "over" then
  update_over()
 elseif mode == "win" then
  update_win()
 end
end

function _draw()
 if mode == "game" then
  draw_game()
 elseif mode == "start" then
  draw_start()
 elseif mode == "wavetxt" then
  draw_wavetxt()
 elseif mode == "over" then
  draw_over()
 elseif mode == "win" then
  draw_win()
 end
end

function startscreen()
 mode = "start"
 music(1)
end

function startgame()
 music(-1, 1000)
 t = 0

 wave = 0
 nextwave()

 ship = {}
 ship.x = 64
 ship.y = 64
 ship.sx = 0
 ship.sy = 0
 ship.spr = 2
 ship.sprw = 1
 ship.sprh = 1

 invul = 0

 flamespr = 5

 bultimer = 0
 bullets = {}
 muzzle = 0

 score = 0

 lives = 2

 stars = {}
 for i = 1, 100 do
  local newstar = {}
  newstar.x = flr(rnd(128))
  newstar.y = flr(rnd(128))
  newstar.spd = rnd(1.5) + 0.5
  add(stars, newstar)
 end

 enemies = {}

 explods = {}

 parts = {}
 hitparts = {}

 shwaves = {}
end

